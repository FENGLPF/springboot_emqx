<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.lpf.dao.MqttMessageDao">

    <insert id="insertMqttMessage" parameterType="com.lpf.model.MqttMessage">
        insert into mqtt_message
        (mqtt_id,topic,message,mqtt_qos,mqtt_received_retained,mqtt_time)
        values
        (#{mqttId},#{topic},#{message},#{mqttQos},#{mqttReceivedRetained},#{mqttTime})
    </insert>

</mapper>